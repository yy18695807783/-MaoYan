                android:background="@drawable/cb_fuwu_selector"
                android:button="@null"
                android:gravity="center"
                android:text="Reald厅"
                android:textColor="@drawable/cb_text_fuwu_selector"
                android:textSize="13sp" />

            <CheckBox
                android:id="@+id/cb_13"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:layout_marginLeft="10dp"
