                android:text="杜比全景声听"
                android:textColor="@drawable/cb_text_fuwu_selector"
                android:textSize="13sp" />

            <CheckBox
                android:id="@+id/cb_15"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
